{
    "workbench.colorTheme": "Dark hacker theme",
    "explorer.confirmDelete": false,
    "editor.wordWrap": "on",
    "editor.renderIndentGuides": true,
    "explorer.confirmDragAndDrop": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.fontFamily": "Dank Mono",
    "BracketHighlighter.textColor": "#ffac0f",
    "BracketHighlighter.border": "0.7px solid rgba(240, 240, 240, 0.418)",
    "BracketHighlighter.useBrackets": true,
    // "BracketHighlighter.fontStyle": "italic",
    "editor.fontLigatures": true,
    "workbench.iconTheme": "eq-material-theme-icons-ocean",
    "workbench.colorCustomizations": {
        "[Dark hacker theme]": {
            "editor.background": "#000000",
            "editor.foreground": "#ffffff",
            "editorLineNumber.foreground": "#ffffff",
            "editorCursor.foreground": "#5ab326",
            "editor.selectionBackground": "#15ff0034",
            "sideBar.background": "#131313",
            "sideBar.border": "#131313",
            "sideBar.dropBackground": "#70707081",
            "sideBarTitle.foreground": "#3fe914",
            "sideBarSectionHeader.background": "#000000",
            "sideBarSectionHeader.border": "#00000023",
            "scrollbar.shadow": "#c0c0c065",
            "scrollbarSlider.activeBackground": "#3fe91483",
            "scrollbarSlider.background": "#ffffff32",
            "scrollbarSlider.hoverBackground": "#ff5e0093",
            "activityBar.background": "#000000",
            "activityBar.foreground": "#ffffff",
            "minimap.findMatchHighlight": "#a6ff00",
            "minimap.errorHighlight": "#ff0000",
            "minimap.warningHighlight": "#ff6600",
            "minimap.selectionHighlight": "#00e1ff",
            "minimap.background": "#0c0c0cd0",
            "editorGroup.border": "#161616",
            "editorGroup.dropBackground": "#00b37d46",
            "tab.unfocusedHoverBackground": "#00b37d46",
            "tab.hoverForeground": "#ffffff",
            "tab.inactiveBackground": "#000000",
            "tab.inactiveForeground": "#fffafa",
            "tab.activeForeground": "#48ff00",
            "tab.activeBackground": "#0f0f0f",
            "tab.border": "#000000",
            "tab.activeBorderTop": "#48ff00",
            "welcomePage.background": "#000000",
            "welcomePage.buttonBackground": "#252525",
            "textLink.foreground": "#51ff00"
        }
    },
    "BracketHighlighter.backgroundColor": "#111111",
    "window.zoomLevel": 0,
    "editor.fontSize": 15,
    "git.autofetch": true,
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.largeFileOptimizations": false
}
